import numpy

def mean(xs):
    n = len(xs)
    x = 0
    i = 0
    while i < n:
        x = x + xs[i]
        i = i + 1
    return float(x)/n

def many_means(ys):
    n = len(ys)
    i = 0
    xs = []
    while i < n:
        m = mean(ys[i])
        xs.append([m])
        i = i + 1
    return xs

